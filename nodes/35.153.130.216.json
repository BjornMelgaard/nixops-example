{
  "environment": "production",
  "users": ["deployer"],
  "authorization": {
    "sudo": {
      "groups": ["deployer", "sysadmin", "www-data"],
      "users": ["deployer"],
      "passwordless": "false"
    }
  },
  "swap_tuning": {
    "minimum_size": 4096
  },
  "run_list": [
    "recipe[swap_tuning]",
    "recipe[sudo]",
    "recipe[users::sysadmins]"
  ],
  "automatic": {
    "ipaddress": "XXX.XXX.XXX.XXX"
  }
}
